    <div  style="float: center; margin-top: 1.5rem;">
      <a type="button" class="btn  btn-lg"  href="../dashboard/get_doc_msgs"><b>Device</b></a>
    </div>
    
    <div class="content">
        <div  style="display:inline-block; vertical-align: top;">
            <img src="../icons/edit_icon.png" alt="img"/>
        </div>
        <div style="display:inline-block;">
           <h3 class="mb-4 pb-2 pb-md-0 mb-md-5" style="color:#007BFF; width: 900;">Device Registration Form</h3>
                  <form method="POST" action="/post-feedback" >
      
                    <div class="row">
                      <div class="col">
                        <div class="form-outline">
                          <input type="text" id="firstName" name="macId" class="form-control" />
                          <label class="form-label" for="firstName" >Device Mac Id</label>
                        </div>
                      </div>
                      <div class="col">
      
                        <div class="form-outline">
                          <input type="text" id="lastName" name="uuId" class="form-control" />
                          <label class="form-label" for="lastName" >Device UUID</label>
                        </div>
      
                      </div>
                    </div>
      
                   
      
                    <div class="row">
                      <div class="col">
      
                        <div class="form-outline">
                            <input type="text" id="lastName" name="lot" class="form-control" />
                            <label class="form-label" for="lastName" >Device Lot</label>
                          </div>
      
                      </div>
                      <div class="col">
      
                        <div class="form-outline">
                            <input type="text" id="lastName" name="model" class="form-control" />
                            <label class="form-label" for="lastName" >Device Model</label>
                          </div>
      
                      </div>
                    </div>
      
                     <div class="row">
                       
                       <div class="col">
      
                        <div class="form-outline">
                            <input type="text" id="lastName" name="devicemodel" class="form-control" />
                            <label class="form-label" for="lastName" >Device Serial Nos.</label>
                          </div>
      
                      </div>
                       
                      <div class="col">
      
                        <select class="select form-control" name="status">
                          <option value="Null">Choose option</option>
                          <option value="Sold">Sold</option>
                          <option value="Demo">Demo</option>
                          <option value="Clinical">Clinical</option>
                          <option value="Development">Development</option>
                          <option value="Production">Production</option>
                        </select>
                        <label class="form-label select-label">Choose option</label>
      
                      </div>
                    </div>
      
                    <div class="mt-4 pt-2">
                      <input class="btn btn-primary btn-lg" type="submit" value="Submit" />
                    </div>
      
                  </form>
    
</div>
